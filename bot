# –° –ø–æ–º–æ—â—å—é –¥–µ–∫–æ—Ä–∞—Ç–æ—Ä–∞ —Å–æ–∑–¥–∞—ë–º –ø–µ—Ä–≤—É—é –∫–æ–º–∞–Ω–¥—É
@tree.command(name = "crit", description = "–ö—Ä–∏—Ç—ã –¥–ª—è –∏–≥—Ä–æ–∫–æ–≤")    
async def crit(ctx,number:int):
    try:
        rez1 = CRITIC[(number+1)//2].split('\n')[1:5] 
        embed = discord.Embed(color=0x8b0000)
        for i in rez1:
            rez2 = i.split(' - ')
            embed.add_field(name=rez2[0], value=rez2[1], inline=False)
        await ctx.response.send_message(f'**–í–∞—à –∫—Ä–∏—Ç: {number}',embed=embed)
    except Exception as e:
        print(f'Error: {e}')

@tree.command(name = "critspell", description = "–ú–∞–≥–∏—á–µ—Å–∫–∏–µ –∫—Ä–∏—Ç—ã")
async def critspell(ctx,number:int):
    try:
        rez1 = CRITIC_MAGIC[number].split('\n')[1:11]
        embed = discord.Embed(color=0x9900CC)
        for i in rez1:
            rez2 = i.split(' - ')
            embed.add_field(name=rez2[0], value=rez2[1], inline=False)
        await ctx.response.send_message(f'–í–∞—à –∫—Ä–∏—Ç: {number}',embed=embed)
    except Exception as e:
        print(f'Error: {e}')

@tree.command(name = "critgm", description = "–ö—Ä–∏—Ç—ã –¥–ª—è –ì–ú–∞")
async def critgm(ctx):
    try:
        number=random.randint(1,101)
        rez1 = CRITIC_GM[(number+1)//2].split('\n')[1:5]
        embed = discord.Embed(color=0xFFFFFF)
        for i in rez1:
            rez2 = i.split(' - ')
            embed.add_field(name=rez2[0], value=rez2[1], inline=False)
        await ctx.response.send_message(f'–í–∞—à –∫—Ä–∏—Ç: {number}',embed=embed)
    except Exception as e:
        print(f'Error: {e}')

@tree.command(name = "anticrit", description = "–ê–Ω—Ç–∏–∫—Ä–∏—Ç—ã")    
async def anticrit(ctx,number:int):
    try:
        rez1 = ANTICRIT[number].split('\n')[1:2] 
        embed = discord.Embed(color=0x8B4513)
        for i in rez1:
            embed.add_field(name=f'–í–∞—à –∞–Ω—Ç–∏–∫—Ä–∏—Ç: {number}',value=i)
        await ctx.response.send_message(embed=embed)
    except Exception as e:
        print(f'Error: {e}')

@bot.event
async def on_message(message):
    try:
        if message.author == bot.user:
            return

        if message.embeds:
            footer = str(message.embeds[0].footer)
            
            if '1–∫100' in footer:
                description = message.embeds[0].description
                value = int(description[1:-1])
                rez1 = CRITIC[(value+1) // 2 ].split('\n')[1:5]

                embed = discord.Embed(color=0x8b0000)
                for i in rez1:
                    rez2 = i.split(' - ')
                    embed.add_field(name=rez2[0], value=rez2[1], inline=False)

                await message.channel.send(embed=embed)
                
        elif 'hello' in message.content:
            await message.channel.send('Hello!')
    
    except Exception as e:
        print(f'Error: {e}')
@bot.event
async def on_ready():
    await tree.sync()
    print("Ready!")
bot.run(config['token'])
